<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>{% block title %}{% endblock %}</title>

<base href="{{ url('ninethousand_jobqueue_home') }}" target="_self" />

{% block stylesheets %}
<link href="{{ asset('/bundles/ninethousandjobqueue/css/style.css') }}" type="text/css" rel="stylesheet" />
<link href="{{ asset('/bundles/ninethousandjobqueue/css/smoothness/jquery-ui-1.8.13.custom.css') }}" type="text/css" rel="stylesheet" />
{% endblock %}

{% block javascripts %}
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.6.2/jquery.min.js" type="text/javascript"></script>
<script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.15/jquery-ui.min.js" type="text/javascript"></script>
<script src="{{ asset('/bundles/ninethousandjobqueue/javascripts/jobqueue.js') }}" type="text/javascript"></script>
<!--[if IE]>
<script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
<![endif]-->
{% endblock %}


<link rel="shortcut icon" href="{{ asset('/bundles/ninethousandjobqueue/favicon.ico') }}" />
</head>

<body class="{{ app.environment }}">
    <div id="wrapper" >
        <div id="leftPanel" >
            {% include "NineThousandJobqueueBundle::menu.html.twig" %}
        </div>

        <div id="rightPanel" >
            {% block body %}{% endblock %}
            <div id="footer" >
                {% include "NineThousandJobqueueBundle::footer.html.twig" %}
            </div>
        </div>
    </div>
</body>
</html>
